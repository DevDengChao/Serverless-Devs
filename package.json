{
  "name": "@serverless-devs/s",
  "version": "3.0.0-beta.3",
  "description": "Serverless devs tool, serverless developer tool, supports Alibaba cloud, AWS, azure, baidu cloud, Huawei cloud, Google cloud and Tencent cloud.",
  "homepage": "https://www.serverless-devs.com",
  "keywords": [
    "serverless",
    "alibaba",
    "tencent",
    "azure",
    "baidu",
    "huawei",
    "google",
    "function",
    "faas",
    "serverless-devs"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Serverless-Devs/Serverless-Devs"
  },
  "bugs": {
    "url": "https://github.com/Serverless-Devs/Serverless-Devs/issues"
  },
  "scripts": {
    "start": "npm run watch",
    "format": "prettier --write src/",
    "watch": "npm run build -- --watch",
    "build": "esbuild src/index.ts --bundle --log-level=error --minify --external:@serverless-devs/core --external:@serverless-devs/ui --platform=node --format=cjs --target=node12 --outfile=lib/index.js",
    "beta": "npm publish --tag=beta",
    "prepkg": "npm run build",
    "pkg": "pkg bin/s"
  },
  "main": "./lib/index.js",
  "bin": {
    "s": "bin/s"
  },
  "devDependencies": {
    "@serverless-devs/credential": "beta",
    "@serverless-devs/engine": "^0.0.1-beta.19",
    "@serverless-devs/load-application": "^0.0.2",
    "@serverless-devs/logger": "^0.0.2-beta.6",
    "@serverless-devs/parse-spec": "^0.0.1-beta.9",
    "@serverless-devs/registry": "beta",
    "@serverless-devs/utils": "^0.0.8-beta.4",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.1",
    "chalk": "^4.1.0",
    "commander": "^11.0.0",
    "esbuild": "^0.18.11",
    "fs-extra": "^11.1.1",
    "global-agent": "^3.0.0",
    "inquirer": "8.2.5",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "pkg": "^5.8.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "table-layout": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "dependencies": {
    "@serverless-devs/core": "^0.1.60"
  }
}
